{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../../../",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^@app/common/core(.*)$": "<rootDir>/common/core/src$1",
    "^@app/common/config(.*)$": "<rootDir>/common/config/src$1",
    "^@app/*": "<rootDir>/apps/*"
  }
}
